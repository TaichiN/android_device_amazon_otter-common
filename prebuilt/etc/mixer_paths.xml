<mixer>
	<!-- These are the initial mixer settings -->
	<!-- general -->
	<ctl name="DL1 Equalizer" value="High-pass 0dB" />
	<ctl name="DL1 Media Playback Volume" value="128" />
	<ctl name="DL1 MM_EXT Switch" value="1" />
	<ctl name="DL1 Mixer Multimedia" value="1" />
	<ctl name="Sidetone Mixer Playback" value="1" />
	<ctl name="Left Output Mixer From DAC_L" value="1" />
	<ctl name="Right Output Mixer From DAC_R" value="1" />
	<ctl name="SP driver mute" value="0" />
	<ctl name="HP driver mute" value="0" />
	<ctl name="M_Input_Mixer MIC1LM_PGA_CNTL" value="0" />
	<ctl name="P_Input_Mixer MIC1LP_PGA_CNTL" value="0" />
	<ctl name="P_Input_Mixer MIC1RP_PGA_CNT" value="0" />
	<ctl name="P_Input_Mixer MIC1LM_PGA_CNTL" value="0" />

	<path name="speaker">
		<ctl name="Class - D driver Volume" value="1" />
		<ctl name="DAC Playback Volume" value="127" />
		<ctl name="SP Analog Gain" value="127" />
		<ctl name="SP driver mute" value="1" />
	</path>
	<path name="headphone">
		<ctl name="Class - D driver Volume" value="1" />
		<ctl name="HPL_PWR_CTL" value="1" />
		<ctl name="HPR_PWR_CTL" value="1" />
		<ctl name="HP Analog Gain" value="127" />
		<ctl name="HP driver mute" value="1" />
	</path>
	<path name="headset">
		<ctl name="Left DAC input selection" value="left data" />
		<ctl name="Right DAC input selection" value="right data" />
		<ctl name="M_Input_Mixer MIC1LM_PGA_CNTL" value="2" />
		<ctl name="P_Input_Mixer MIC1LP_PGA_CNTL" value="2" />
		<ctl name="P_Input_Mixer MIC1RP_PGA_CNT" value="2" />
		<ctl name="P_Input_Mixer MIC1LM_PGA_CNTL" value="2" />
		<ctl name="Sidetone Mixer Capture" value="1" />
	</path>
</mixer>
